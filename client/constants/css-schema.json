{
  "label":
  {
    "control-label":
    {
      "font-weight": "700",
      "font-size": "14px",
      // "height": "34px",
      // "line-height": "34px",
      "background-color": "white",
      "color": "#333",
      "border-left": "none",
      "border-right": "none",
      "border-top": "none",
      "border-bottom": "none",
      "border": "none",
      // "padding-left": "0px",
      "padding-right": "0px",
      "padding-top": "0px",
      // "padding-bottom": "0px"
    }
  },
  "box":
  {
    "selectors":
    {
      "form-control":
      {
        // "height": "34px",
        // "line-height": "34px",
        "background-color": "white",
        "color": "#333",
        "border-left": "none",
        "border-right": "none",
        "border-top": "none",
        "border-bottom": "none",
        "border": "1px solid #ccc",
        "border-radius": "4px",
        // "padding-left": "0px",
        "padding-right": "0px",
        "padding-top": "0px",
        // "padding-bottom": "0px"
      },
      ".form-radio-group .radio label span, .checkboxes .checkbox label span":
      {
        // "width": "",
        // "height": "20px",
        // "line-height": "20px",
        "background-color": "white",
        "color": "#333",
        "border-left": "none",
        "border-right": "none",
        "border-top": "none",
        "border-bottom": "none",
        "border": "1px solid #ccc",
        "border-radius": "4px",
        // "padding-left": "0px",
        "padding-right": "0px",
        "padding-top": "0px",
        // "padding-bottom": "0px"
      }
    },
    "pseudo":
    {
      ".form-group .field-radio-group .radio label span":
      {
        "::before":
        {
          "border": "1px solid #555",
          "border-radius": "3px",
          "background-color": "#fff",
        },
        "::after":
        {
          "color": "#555555"
        }
      },
      ".form-group .checkboxes .checkbox label span":
      {
        "::before":
        {
          "border": "1px solid #555",
          "border-radius": "3px",
          "background-color": "#fff",
        },
        "::after":
        {
          "color": "#555555"
        }
      }
    }
  }
}
